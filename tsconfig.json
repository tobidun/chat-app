{
  "compilerOptions": {
    "target": "es2016",
    "jsxImportSource": "js",
    "module": "commonjs",
    "outDir": "./dist",
    "inlineSourceMap": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "experimentalDecorators": true
  }
}